    <layout type="yoga" width="fill" height="fill">
        <list backgroundColor="#EEEEEE" bottomToBottom="parent" id="stud_list" marginBottom="0dp" marginTop="0dp" orientation="vertical" src="${db_list}" topToTop="parent" width="100%" height="100%">
            <layout type="yoga" payload="header">
                <text gravity="center" height="60dp"  src="I'm header View" width="fill" />
            </layout>
            <layout type="yoga" payload="vh">
                <view backgroundColor="#666666" height="1px" width="100%" />
                <!-- iOS侧百分比支持有问题，使用yoga套娃一层 -->
                <layout positionType="absolute" positionLeft="20dp" positionTop="0dp" positionBottom="0dp" type="yoga" payload="vh" justify-content = "center" width="40dp">
                    <image src="${img}" width="40dp" height="40dp" />
                </layout>
                <text style="bold" color="#333333" marginLeft="72dp" marginTop="10dp" size="16dp" src="${title}" />
                <text color="#999999" marginLeft="72dp" marginTop="8dp" marginBottom="10dp" size="12dp" src="${subtitle}" />
                <layout positionType="absolute"  positionRight="30dp" aspectRatio="1" positionTop="0dp" positionBottom="0dp" type="yoga" payload="vh" justify-content = "center" width="40dp">
                    <image src="${arrow}" height="10dp" />
                </layout>
            </layout>
            <layout type="yoga" payload="footer">
                <view backgroundColor="#666666" height="1px" id="line" topToTop="parent" width="fill" />
                <text gravity="center" height="60dp" src="I'm footer View" topToBottom="line" width="fill" />
            </layout>
        </list>
    </layout>
